<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
          "http://www.w3.org/TR/html4/loose.dtd">

<html lang="en">
  <head>
    <title>[% page_title %]</title>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <style type="text/css" media="screen">
      [% style %]
    </style>
  </head>
  <body>
    <div class="container">
      <div class="background">
        <h1 class="report_title">[% page_title %]</h1>
        [% report_content %]
        <table id="the_table">
            <thead>
                <tr>
                    <th>1</th>
                    <th>2</th>
                    <th>3</th>
                    <th>4</th>
                    <th>5</th>
                    <th>6</th>
                    <th>7</th>
                    <th>8</th>
                    <th>9</th>
                    <th>10</th>
                    <th>11</th>
                    <th>12</th>
                    <th>13</th>
                    <th>14</th>
                    <th>15</th>
                    <th>16</th>
                    <th>17</th>
                    <th>18</th>
                    <th>19</th>
                    <th>20</th>
                    <th>21</th>
                </tr>
            </thead>
            <tbody>
                [% FOREACH row = lines %]
                <tr>
                    [% FOREACH item = row %]
                        <td>[% item %]</td>
                    [% END %]
                </tr>
                [% END %]
            </tbody>
        </table>
      </div>&nbsp;
    </div>
  </body>
</html>
