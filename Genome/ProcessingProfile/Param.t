#!/gsc/bin/perl

use strict;
use warnings;

$ENV{UR_DBI_NO_COMMIT} = 1;
$ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;

use Test::More tests => 4;

use above "Genome";

my $pp_id = '1765057';
my $name = 'test_param';
my $value = 'taco';

my $pp_param = Genome::ProcessingProfile::Param->create(
                                                        processing_profile_id => $pp_id,
                                                        name => $name,
                                                        value => $value,
);
isa_ok($pp_param,'Genome::ProcessingProfile::Param');
is($pp_param->name,$name,'name accessor');
is($pp_param->value,$value,'value accessor');

UR::Context->commit;
exit;
