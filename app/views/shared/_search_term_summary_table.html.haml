.row-fluid
  .span12
    .well
      %h3 Search Term Summary
      %table.table.table-striped.table-bordered(cellpadding="0" cellspacing="0" border="0")
        %thead
          %tr
            %th Search Term
            %th Match Type
            %th Matches
        %tbody
          -results.each_result do |result|
            %tr
              %td=result.search_term
              %td=result.match_type
              %td=result.matches
